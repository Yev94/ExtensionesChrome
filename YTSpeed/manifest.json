{
    "name": "YTSpeed",
    "description": "Pon la velocidad doble autom√°ticamente",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "content_scripts" : [{
        "js" : ["content.js"],
        "matches" : ["https://www.youtube.com/watch*"],
        "css": ["style.css"]
    }],
    "action" : {
        "default_popup" : "popup.html"
    },
    "permissions": ["storage", "scripting", "tabs", "activeTab", "notifications"],
    "host_permissions": ["<all_urls>"],
    "web_accessible_resources": [
        {
            "resources": ["modules/changeYTSpeed.js" ],
            "matches": [ "<all_urls>" ]
        }
    ]
}